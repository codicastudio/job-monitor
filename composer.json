{
    "name" : "codica-studio/jobmonitor",
    "description": "A random Codica Studio package.",

    "homepage": "https://github.com/codica-studio/jobmonitor",
    "license": "MIT",
    "authors": [
        {
            "name": "Michael J Brown",
            "email": "mbrown@codicastudio.com",
            "homepage": "https://codicastudio.com",
            "role": "Developer"
        }
    ],
    "require" : {
        "php": "^7.4 || ^8.0",
        "laravel/framework" : "^8.0"
    },
    "require-dev" : {
        "phpunit/phpunit" : "^8.0",
        "orchestra/testbench" : "^6.0"
    },
    "autoload" : {
        "psr-4" : {
            "Spatie\\FailedJobMonitor\\" : "src"
        }
    },
    "autoload-dev" : {
        "psr-4" : {
            "Spatie\\FailedJobMonitor\\Test\\" : "tests"
        }
    },
    "scripts" : {
        "test" : "vendor/bin/phpunit"
    },
    "suggest" : {
        "laravel/slack-notification-channel" : "Required for sending notifications via Slack"
    },
    "extra" : {
        "laravel" : {
            "providers" : [
                "Spatie\\FailedJobMonitor\\FailedJobMonitorServiceProvider"
            ]
        }
    }
}
